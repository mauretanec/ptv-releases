{
  "version": "1.0.0-*",
  "description": "PTV.Application.Api Class Library",
  "authors": [ "" ],
 "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dnxcore50",
        "netcore50"
      ]
    }
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.1.0",
      "type": "platform"
    },
"Microsoft.EntityFrameworkCore.Tools": "1.1.0-preview4-final",
    "Npgsql.EntityFrameworkCore.PostgreSQL": "1.1.0",
    "Npgsql.EntityFrameworkCore.PostgreSQL.Design": "1.1.0",
    "Microsoft.AspNetCore.Mvc": "1.1.0",
    "Microsoft.AspNetCore.Identity": "1.1.0",
    "Microsoft.AspNetCore.Hosting": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.Extensions.Options": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.1.0",
    "Microsoft.Extensions.Configuration.UserSecrets": "1.1.0",
    "Microsoft.Extensions.Configuration.Binder": "1.1.0",
    "Microsoft.AspNetCore.StaticFiles": "1.1.0",
    "Microsoft.AspNetCore.Authentication.OpenIdConnect": "1.1.0",
    "Microsoft.AspNetCore.Authentication.Cookies": "1.1.0",
    "Microsoft.AspNetCore.Authentication.JwtBearer": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Microsoft.Extensions.Logging.Debug": "1.1.0",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Diagnostics.EntityFrameworkCore": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.AspNetCore.DataProtection": "1.1.0",
    "Microsoft.AspNetCore.Authentication.OAuth": "1.1.0",
    "Microsoft.AspNetCore.Mvc.Formatters.Json": "1.1.0",
    "IdentityServer4.AccessTokenValidation": "1.0.1-beta2",
    "NLog.Extensions.Logging": "1.0.0-rtm-alpha4",
    "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0",
    "Microsoft.VisualStudio.Web.CodeGeneration.Tools": {
      "version": "1.1.0-preview4-final",
      "type": "build"
    },
    "Microsoft.VisualStudio.Web.CodeGenerators.Mvc": {
      "version": "1.1.0-preview4-final",
      "type": "build"
    },
    "Microsoft.AspNetCore.Razor.Tools": {
      "version": "1.1.0-preview4-final",
      "type": "build"
    },
    "PTV.Database.DataAccess.Interfaces": "1.0.0-*",
    "PTV.Database.DataAccess": "1.0.0-*",
    "PTV.Domain.Model": "1.0.0-*",
    "PTV.Framework": "1.0.0-*",
    "PTV.Application.Framework": "1.0.0-*",
    "System.Net.Http": "4.3.0",
    "Microsoft.AspNetCore.Http.Abstractions": "1.1.0",
    "Swashbuckle.AspNetCore": "1.0.0-rc1",
    "Swashbuckle.AspNetCore.SwaggerUi": "1.0.0-rc1"
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "xmlDoc": true,
    "nowarn": [ "1587" ],
    "warningsAsErrors": true,
    "preserveCompilationContext": true,
    "copyToOutput":{
      "mappings": {
        "MapServiceRequestBody.xml": "../PTV.Framework/MapServiceRequestBody.xml"
      }
    }
  },

  "runtimeOptions": {
    "gcServer": true
  },

  "publishOptions": {
    "mappings": {
      "MapServiceRequestBody.xml": "../PTV.Framework/MapServiceRequestBody.xml"
    },
    "include": [
      "wwwroot",
      "Views",
      "appsettings.json",
      "web.config",
      "nlog.config",
      "MapServiceRequestBody.xml",
	  "PTV.Application.Api.xml",
	  "PTV.Domain.Model.xml"
    ],
    "exclude": [ "node_modules" ]
  },

  "scripts": {
    "prepublish": [ ],
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }

}
