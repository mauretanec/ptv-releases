{
  "version": "1.0.0-*",
  "description": "PTV.DataImport.ConsoleApp development tool to import JSON format data from database to PTV database.",
  "authors": [ "" ],
    "frameworks": {
      "netcoreapp1.1": {
        "imports": [
          "dnxcore50",
          "netcore50"
        ]
      }
    },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "version": "1.1.0",
      "type": "platform"
    },
    "Microsoft.EntityFrameworkCore.Tools": "1.1.0-preview4-final",
    "Npgsql.EntityFrameworkCore.PostgreSQL": "1.1.0",
    "Npgsql.EntityFrameworkCore.PostgreSQL.Design": "1.1.0",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Configuration.CommandLine": "1.1.0",
    "PTV.Database.DataAccess": "1.0.0-*",
    "PTV.Database.DataAccess.Interfaces": "1.0.0-*",
    "PTV.ExternalSources": "1.0.0-*",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "NLog.Extensions.Logging": "1.0.0-rtm-alpha2",
    "Microsoft.EntityFrameworkCore.Relational": "1.1.0",
    "Microsoft.Extensions.Logging": "1.1.0",
    "Microsoft.EntityFrameworkCore": "1.1.0",
    "PTV.Application.Framework": "1.0.0-*"
  },

    "tools": {},


  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": [ "appsettings.json", "appsettings.Custom.json", "appsettings.Development.json" ]
  },

    "runtimeOptions": {
      "gcServer": true
    },

    "publishOptions": {
        "include": [
            "appsettings.json",
            "Generated",
            "SqlMigrations"
        ],
        "exclude": [ "node_modules" ]
    }

}
